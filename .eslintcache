/src/pages/LogIn.jsx",[],"/Users/francisco/ironhack/last-project-react/MagicTG-Client/src/pages/Event/SingleEvent.jsx",[],"/Users/francisco/ironhack/last-project-react/MagicTG-Client/src/pages/SignUp/Signup.jsx",[],"/Users/francisco/ironhack/last-project-react/MagicTG-Client/src/pages/HomePage/HomePage.jsx",[],"/Users/francisco/ironhack/last-project-react/MagicTG-Client/src/pages/User/EditPlayer.jsx",[],"/Users/francisco/ironhack/last-project-react/MagicTG-Client/src/pages/User/EditProfileOrganizer.jsx",[],"/Users/francisco/ironhack/last-project-react/MagicTG-Client/src/pages/User/PlayerProfile.jsx",[],"/Users/francisco/ironhack/last-project-react/MagicTG-Client/src/routing-components/NormalRoute.jsx",[],"/Users/francisco/ironhack/last-project-react/MagicTG-Client/src/pages/User/OrganizerProfile.jsx",[],"/Users/francisco/ironhack/last-project-react/MagicTG-Client/src/routing-components/ProtectedRoute.jsx",[],"/Users/francisco/ironhack/last-project-react/MagicTG-Client/src/components/Navbar/Navbar.jsx",[],"/Users/francisco/ironhack/last-project-react/MagicTG-Client/src/pages/Event/AllEvents.jsx",["127"],"import React, { useEffect, useState, useRef } from \"react\";\nimport { getAllEvents } from \"../../services/events\";\nimport \"./Allevents.css\";\nimport { Link } from \"react-router-dom\";\nimport LoadingComponent from \"../../components/Loading\";\nimport ReactMapGL, { Marker } from \"react-map-gl\";\nimport Geocoder from \"react-map-gl-geocoder\";\nimport \"react-map-gl-geocoder/dist/mapbox-gl-geocoder.css\";\nimport \"./Allevents.css\";\nimport turf from \"turf\";\nimport EventCard from \"../../components/EventCard\";\n\nconst AllEvents = (props) => {\n  const [userPosition, setUserPosition] = useState([]);\n  const [events, setEvents] = useState([]);\n  const [loading, setLoading] = useState(true);\n  const [viewport, setViewport] = useState({\n    latitude: 41.1496,\n    longitude: -8.61099,\n    zoom: 0,\n    bearing: 0,\n    pitch: 0,\n  });\n\n  const mapRef = useRef();\n  const geocoderContainerRef = useRef();\n\n  useEffect(() => {\n    getAllEvents().then((events) => {\n      navigator.geolocation.getCurrentPosition((position) => {\n        const userPosition = [\n          position.coords.latitude,\n          position.coords.longitude,\n        ];\n        setUserPosition(userPosition);\n\n        const eventsWithDistance = events.map((event) => {\n          const arrayPlayersId = event.players.map((player) => player._id);\n\n          let from = {\n            type: \"Feature\",\n            properties: {},\n            geometry: {\n              type: \"Point\",\n              coordinates: userPosition,\n            },\n          };\n          let to = {\n            type: \"Feature\",\n            properties: {},\n            geometry: {\n              type: \"Point\",\n              coordinates: [event.lat, event.long],\n            },\n          };\n          let distance = turf.distance(from, to, \"kilometers\");\n          return {\n            ...event,\n            distance: distance,\n            going: event.players.length,\n            arrayPlayersId: arrayPlayersId,\n          };\n        });\n        setEvents(eventsWithDistance);\n\n        setLoading(false);\n      });\n    });\n  }, []);\n\n  return (\n    <div className=\"eventsPage\">\n      <div className=\"leftPannel\">\n        {loading ? (\n          <LoadingComponent />\n        ) : (\n          <>\n            <h2>Closest to Me</h2>\n            <div className=\"cardRow\">\n              {events\n                .sort((a, b) => a.distance - b.distance)\n                .map((el) => (\n                  <Link key={el._id} to={`/event/${el._id}`}>\n                    <EventCard\n                      UserGoing={el.arrayPlayersId.includes(props.user._id)}\n                      Distance={el.distance}\n                      Id={el._id}\n                      Name={el.name}\n                      Going={el.going}\n                      Format={el.format}\n                      DateHour={new Date(el.date)\n                        .toISOString()\n                        .substring(12, 16)}\n                      DateDay={new Date(el.date).toISOString().substring(0, 10)}\n                      Image={el.image}\n                    />\n                  </Link>\n                ))}\n            </div>\n            <h2>Trending</h2>\n            <div className=\"cardRow\">\n              {events\n                .sort((a, b) => b.going - a.going)\n                .map((el) => (\n                  <Link key={el._id} to={`/event/${el._id}`}>\n                    <EventCard\n                      Distance={el.distance}\n                      Id={el._id}\n                      Name={el.name}\n                      Going={el.going}\n                      Format={el.format}\n                      DateHour={new Date(el.date)\n                        .toISOString()\n                        .substring(12, 16)}\n                      DateDay={new Date(el.date).toISOString().substring(0, 10)}\n                      Image={el.image}\n                    />\n                  </Link>\n                ))}\n            </div>\n            <h2>Starting Soon</h2>\n            <div className=\"cardRow\">\n              {events\n                .sort((a, b) => new Date(a.date) - new Date(b.date))\n                .map((el) => (\n                  <Link key={el._id} to={`/event/${el._id}`}>\n                    <EventCard\n                      Distance={el.distance}\n                      Id={el._id}\n                      Name={el.name}\n                      Going={el.going}\n                      Format={el.format}\n                      DateHour={new Date(el.date)\n                        .toISOString()\n                        .substring(12, 16)}\n                      DateDay={new Date(el.date).toISOString().substring(0, 10)}\n                      Image={el.image}\n                    />\n                  </Link>\n                ))}\n            </div>\n          </>\n        )}\n      </div>\n\n      <div className=\"rightPannel\">\n        <h2>The Real Gathering Sights</h2>\n        <div className=\"mapbox\">\n          <div\n            ref={geocoderContainerRef}\n            style={{\n              position: \"relative\",\n              top: 20,\n              left: 0,\n              zIndex: 0,\n            }}\n          />\n          <ReactMapGL\n            {...viewport}\n            ref={mapRef}\n            onViewportChange={(nextViewport) => setViewport(nextViewport)}\n            width=\"100%\"\n            height=\"400px\"\n            mapStyle={\"mapbox://styles/xikz/ckirobijq062z19mhitx2z914\"}\n            mapboxApiAccessToken={\n              \"pk.eyJ1IjoieGlreiIsImEiOiJja2luMWxod3owa2VrMnhxczF3cHo0Y2FpIn0.6EG6l8fbS8yp3vNXmZBJlA\"\n            }\n          >\n            <Geocoder\n              mapRef={mapRef}\n              onViewportChange={(nextViewport) => setViewport(nextViewport)}\n              mapboxApiAccessToken={\n                \"pk.eyJ1IjoieGlreiIsImEiOiJja2luMWxod3owa2VrMnhxczF3cHo0Y2FpIn0.6EG6l8fbS8yp3vNXmZBJlA\"\n              }\n              position=\"top-left\"\n            />\n\n            {events &&\n              events.map((event) => (\n                <Marker\n                  key={event._id}\n                  latitude={Number(event.lat)}\n                  longitude={Number(event.long)}\n                  offsetLeft={-20}\n                  offsetTop={0}\n                >\n                  <div className=\"eventImage\">\n                    <img src=\"../../../images/eventicon.png\" alt=\"event icon\" />\n                  </div>\n                </Marker>\n              ))}\n          </ReactMapGL>\n        </div>\n      </div>\n    </div>\n  );\n};\nexport default AllEvents;\n","/Users/francisco/ironhack/last-project-react/MagicTG-Client/src/components/Loading/index.jsx",[],"/Users/francisco/ironhack/last-project-react/MagicTG-Cl